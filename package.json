        "@commitlint/config-conventional": "^17.1.0",
        "@semantic-release/changelog": "^6.0.1",
        "@semantic-release/git": "^10.0.1",
        "glob": "^7.2.3",
        "husky": "^8.0.1",
        "jest": "^29.4.1",
        "jest-junit": "^15.0.0",
        "npm-run-all": "^4.1.5",
        "sass-true": "^7.0.0",
        "semantic-release": "^19.0.5",
        "stylelint": "^14.14.0",
        "stylelint-config-unsass": "^1.0.1"
    },
    "dependencies": {
        "@unsass/breakpoint": "^2.2.0",
        "@unsass/css": "^2.2.0",
