{
    "name": "@magnesium/theme",
    "version": "4.4.1-beta.3",
    "description": "The Sass Framework for Web Design System.",
    "scripts": {
        "lint:css": "stylelint --fix \"**/*.scss\"",
        "lint": "npm-run-all --parallel lint:*",
        "test": "jest",
        "prepare": "husky",
        "release:beta": "release-it --preRelease=beta",
        "release": "release-it"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "sass": "index.scss",
    "exports": {
        ".": {
            "sass": "./index.scss"
        },
        "./color": {
            "sass": "./color.scss"
        },
        "./shape": {
            "sass": "./shape.scss"
        }
    },
    "devDependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@release-it/conventional-changelog": "^8.0.2",
        "glob": "^7.2.3",
        "husky": "^9.1.6",
        "jest": "^29.7.0",
        "jest-environment-node-single-context": "^29.4.0",
        "jest-junit": "^16.0.0",
        "npm-run-all": "^4.1.5",
        "release-it": "^17.7.0",
        "sass-true": "^8.1.0",
        "stylelint": "^15.8.0",
        "stylelint-config-unsass": "^1.0.1"
    },
    "dependencies": {
        "@unsass/css": "^2.3.0",
        "@unsass/string": "^1.4.1",
        "@unsass/types": "^1.0.1",
        "@unsass/var": "^1.0.2",
        "sass": "^1.79.4"
    },
    "keywords": [
        "sass",
        "framework",
        "design-system",
        "theme",
        "color",
        "shape",
        "magnesium",
        "front-end"
    ],
    "license": "MIT",
    "homepage": "https://github.com/magnesiumlabs/magnesium",
    "bugs": {
        "url": "https://github.com/magnesiumlabs/magnesium/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/magnesiumlabs/magnesium.git"
    },
    "files": [
        ".github/banner.svg",
        "src/**/*",
        "*.scss",
        "CHANGELOG.md"
    ],
    "publishConfig": {
        "access": "public"
    }
}
