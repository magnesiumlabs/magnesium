{
    "name": "@magnesium/theme",
    "version": "3.6.0",
    "description": "The Sass Framework for Web Design System.",
    "scripts": {
        "lint:css": "stylelint --fix \"**/*.scss\"",
        "lint": "npm-run-all --parallel lint:*",
        "test": "jest",
        "prepare": "husky install",
        "release": "semantic-release"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "sass": "index.scss",
    "devDependencies": {
        "@commitlint/cli": "^17.6.1",
        "@commitlint/config-conventional": "^17.6.1",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "glob": "^7.2.3",
        "husky": "^8.0.1",
        "jest": "^29.5.0",
        "jest-environment-node-single-context": "^29.0.0",
        "jest-junit": "^15.0.0",
        "npm-run-all": "^4.1.5",
        "sass-true": "^7.0.0",
        "semantic-release": "^21.0.1",
        "stylelint": "^15.4.0",
        "stylelint-config-unsass": "^1.0.1"
    },
    "dependencies": {
        "@unsass/css": "^2.3.0",
        "@unsass/em": "^2.1.0",
        "@unsass/rem": "^2.1.0",
        "@unsass/string": "^1.4.1",
        "@unsass/types": "^1.0.1",
        "@unsass/var": "^1.0.2",
        "sass": "^1.62.0"
    },
    "keywords": [
        "sass",
        "design-system",
        "framework",
        "theme",
        "color",
        "typography",
        "shape",
        "magnesium",
        "front-end"
    ],
    "license": "MIT",
    "homepage": "https://github.com/magnesiumlabs/magnesium",
    "bugs": {
        "url": "https://github.com/magnesiumlabs/magnesium/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/magnesiumlabs/magnesium.git"
    },
    "files": [
        ".github/banner.svg",
        "color/**/*",
        "core/**/*",
        "shape/**/*",
        "state/**/*",
        "theme/**/*",
        "typography/**/*",
        "*.scss",
        "CHANGELOG.md"
    ],
    "publishConfig": {
        "access": "public"
    }
}
